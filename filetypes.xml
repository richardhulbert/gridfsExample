<application>
  <component name="FileTypeManager" version="17">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <mapping pattern=".env_example" type=".env file" />
      <mapping pattern="Docker" type="Dockerfile" />
      <mapping pattern="versions" type="PLAIN_TEXT" />
      <mapping ext="template" type="PLAIN_TEXT" />
      <mapping ext="dat" type="PLAIN_TEXT" />
      <mapping pattern="_elements" type="SCSS" />
    </extensionMap>
  </component>
</application>